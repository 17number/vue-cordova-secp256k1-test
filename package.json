    "prettier": "^1.19.1",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "typescript": "~3.5.3",
    "vue-cli-plugin-cordova": "^2.3.7",
    "vue-template-compiler": "^2.6.10"
